{
  "name": "nmr-processing",
  "version": "3.0.0",
  "description": "Pure functions allowing to process NMR spectra.",
  "main": "./lib/index.js",
  "module": "./lib-esm/index.js",
  "types": "./lib/index.d.ts",
  "keywords": [],
  "author": "Luc Patiny",
  "license": "MIT",
  "files": [
    "types",
    "src",
    "lib",
    "lib-esm"
  ],
  "scripts": {
    "clean": "rimraf lib lib-esm",
    "build": "npm run tsc && cheminfo-build",
    "eslint": "eslint src --ext ts --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "prepublishOnly": "npm run tsc",
    "test": "npm run test-coverage && npm run eslint",
    "test-coverage": "npm run test-only -- --coverage",
    "test-only": "jest",
    "tsc": "npm run clean && npm run tsc-cjs && npm run tsc-esm",
    "tsc-cjs": "tsc --project tsconfig.cjs.json",
    "tsc-esm": "tsc --project tsconfig.esm.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/nmr-processing.git"
  },
  "bugs": {
    "url": "https://github.com/cheminfo/nmr-processing/issues"
  },
  "homepage": "https://github.com/cheminfo/nmr-processing#readme",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "cheminfo-build": "^1.1.11",
    "cheminfo-types": "^0.3.1",
    "eslint": "^7.32.0",
    "eslint-config-cheminfo-typescript": "^8.0.9",
    "esm": "^3.2.25",
    "jest": "^27.0.6",
    "jest-matcher-deep-close-to": "^2.0.1",
    "md5": "^2.3.0",
    "ml-array-median": "^1.1.5",
    "nmr-xy-testdata": "^0.2.1",
    "openchemlib": "^7.4.2",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "assign-deep": "^1.0.1",
    "binary-search": "^1.3.6",
    "cross-fetch": "^3.1.4",
    "form-data": "^4.0.0",
    "is-any-array": "^1.0.0",
    "ml-array-mean": "^1.1.5",
    "ml-array-rescale": "^1.3.6",
    "ml-array-sum": "^1.1.5",
    "ml-gsd": "^6.8.1",
    "ml-hclust": "^3.0.0",
    "ml-levenberg-marquardt": "^3.1.1",
    "ml-matrix": "^6.8.0",
    "ml-matrix-convolution": "^1.0.0",
    "ml-matrix-peaks-finder": "^1.0.0",
    "ml-peak-shape-generator": "^2.0.2",
    "ml-simple-clustering": "^0.1.0",
    "ml-sparse-matrix": "^2.1.0",
    "ml-spectra-processing": "^6.8.0",
    "ml-tree-set": "^0.1.1",
    "openchemlib-utils": "^1.5.0",
    "spectrum-generator": "^5.4.0"
  }
}
